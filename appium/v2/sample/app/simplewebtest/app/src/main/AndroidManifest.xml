<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simplewebtest">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">

        <activity android:name="com.example.simplewebtest.MainActivity" android:label="@string/app_name" android:theme="@android:style/Theme.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.example.simplewebtest.ListActivity" android:label="@string/list_header_label" android:theme="@android:style/Theme.Light"/>
        <activity android:name="com.example.simplewebtest.WebActivity" android:label="@string/web_header_label" android:theme="@android:style/Theme.Light"/>

    </application>

</manifest>
